{
  "name": "yt-util",
  "description": "Some utilities to improve the YT browsing experience",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Enable TY utils!"
  },
  "permissions": [
    "storage",
    "tabs",
    "https://www.youtube.com/feed/subscriptions"
  ]
}
